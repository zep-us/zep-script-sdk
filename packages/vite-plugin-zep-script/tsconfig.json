{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",
    "lib": ["ESNext"],
    "module": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "dist"
  ]
}
